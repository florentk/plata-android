<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/labelMyPos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/myPos" />

    <TextView
    android:id="@+id/dataMyPos"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/noVehicle"
    android:layout_below="@+id/labelMyPos"
    android:layout_alignLeft="@+id/labelMyPos"/>

    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Send position"
            android:id="@+id/buttonSend"
            android:layout_below="@+id/dataMyPos"
            android:layout_alignLeft="@+id/dataMyPos"
            android:layout_marginTop="20dp"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/neighbour"
            android:layout_below="@+id/buttonSend"
            android:layout_alignLeft="@+id/buttonSend"
            android:layout_marginTop="14dp"
            android:text="Pas de voisins"
            android:phoneNumber="true"/>

    <view
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            class="fr.ifsttar.plaiimob.BikeView"
            android:id="@+id/bikeView"
            android:layout_below="@+id/neighbour"
            android:layout_marginTop="11dp"
            android:layout_alignLeft="@+id/neighbour"/>


</RelativeLayout>
